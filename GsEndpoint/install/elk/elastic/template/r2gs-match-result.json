      "PGS_KILLS": {
        "type": "integer"
      },
      "PGS_ASSAULT_SCORE": {
        "type": "integer"
      },
      "PGS_DEFENSE_SCORE": {
        "type": "integer"
      },
      "PGS_DISTANCE_SCORE": {
        "type": "integer"
      },
      "PGS_DETONATION_SCORE": {
        "type": "integer"
      },
      "PGS_DEATHS": {
        "type": "integer"
      },
      "PGS_NPC_KILLS": {
        "type": "integer"
      },
