{
  "version": 3,
  "index_patterns": [
    "r2gs-player-uid-*"
  ],
  "aliases": {
    "r2gs-player-uid": {}
  },
  "settings": {
    "number_of_shards": "1",
    "auto_expand_replicas": "0-1"
  },
  "mappings": {
    "properties": {
      "playerName": {
        "type": "keyword"
      },
      "uId": {
        "type": "keyword"
      },
      "lastUpdate": {
        "type": "date",
        "format": "strict_date_optional_time_nanos||epoch_millis"
      }
    }
  }
}